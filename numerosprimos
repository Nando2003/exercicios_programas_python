numero = int(input("Insira um numero: "))

if numero > 1:
    for x in range(2, numero):
        if (numero % x) == 0:
            print("Numero nao eh primo")
            break
        else:
            print("Numero eh primo")
            break
else:
    print("Numero nao eh primo")

# se resto != 0 para qualquer numero que nao seja ele, o numero eh primo
